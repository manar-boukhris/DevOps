pipeline {
    agent any

    stages {
        stage('Checkout from Git') {
            steps {
                echo 'Pulling from Git...'
                git branch: 'Ismail', url: 'https://github.com/ismailgharnougui/Devops'
            }
        }

        stage('Testing Maven') {
            steps {
                echo 'Testing Maven...'
                sh 'mvn -version'
            }
        }

    }
}